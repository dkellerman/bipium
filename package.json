{
  "name": "@dkellerman/bipium",
  "version": "1.0.1",
  "private": true,
  "author": "David Kellerman <david@silvermoon.org>",
  "license": "MIT",
  "homepage": "https://bipium.com",
  "dependencies": {
    "@pixi/react": "^8.0.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "copy-to-clipboard": "^3.3.3",
    "local-storage-fallback": "^5.0.0",
    "lucide-react": "^0.575.0",
    "pixi.js": "^8.16.0",
    "query-string": "^9.3.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.13.1",
    "sessionstorage": "^0.1.0",
    "standardized-audio-context": "^25.3.72",
    "tailwind-merge": "^3.5.0",
    "tailwindcss": "^4.2.1"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.2.1",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "jsdom": "^27.0.1",
    "prettier": "^3.8.1",
    "regenerator-runtime": "^0.14.1",
    "standardized-audio-context-mock": "^9.7.26",
    "typescript": "^5.9.3",
    "vercel": "^50.25.1",
    "vite": "^7.3.1",
    "vitest": "^3.2.4"
  },
  "prettier": {
    "printWidth": 100,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "jsxSingleQuote": false,
    "trailingComma": "all",
    "arrowParens": "avoid"
  },
  "scripts": {
    "vercel": "node_modules/.bin/vercel",
    "dev": "vite",
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run --environment jsdom",
    "test:watch": "vitest --environment jsdom",
    "build:core": "vite build --mode core-unminified && vite build --mode core-minified",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,md}\" index.html package.json tsconfig.json vite.config.mjs",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css,md}\" index.html package.json tsconfig.json vite.config.mjs",
    "typecheck": "tsc --noEmit",
    "lint": "npm run typecheck"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "packageManager": "yarn@1.22.1"
}
